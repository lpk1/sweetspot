<div class="card-listing">
  <div class="card-category-listing"><%= listing.name %></div>
  <div class="card-description-listing">
    <p><%= listing.description %></p>
    <br>

    <p>Address:  <%= listing.address %></p>
    <p>Price: FREE</p>
    <p>3.1 ğŸ’šğŸ’šğŸ’šğŸ–¤ğŸ–¤</p>
  </div>



  <img class="main_image-listing" src="<%= cl_image_path listing.photo %>">



  <img class="card-user-listing" src="https://www.fabulousfaces.com/lib/upload/lrs1595.jpg">


  <% if listing.user == current_user %>
  <p class= "edit-listing" style= "z-index: 3;"><%= link_to '<i class="fas fa-pencil-alt"></i>'.html_safe,  edit_spot_path(listing), style: 'color:#5A5A5A'%></p>
  <% end %>
  <% if listing.user == current_user %>
  <p class= "delete-listing" style= "z-index: 3;"><%= link_to '<i class="fas fa-trash"></i>'.html_safe,  spot_path(listing), style: 'color:#5A5A5A', :method => :delete %></p>
  <% end %>

