    <div class="map_img-listing map-fullscreen">
        <div
          id="map"
          style="width: 100vw;
          height: 35vh;
          border-radius: 5px"

          data-markers="<%= @markers.to_json %>"
          data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>">
        </div>
    </div>


<div class="container">
  <% if @listings.length != 0 %>
    <div class="listings-title">
      <h1 class= "listing-page-title">Your Listings:</h1>
      <h1 class="add-spot", style="text-align: right;"><%= link_to '<i class="fas fa-plus" style="color: #5a5a5a"></i>'.html_safe, new_spot_path %></h1>
    </div>
    <% @listings.each do |listing| %>
      <br>
      <%= render "shared/listing_card", listing: listing %>
    <% end %>
  <% else %>
    <h3>You don't have any listings so far. Go ahead,</h3>
    <h1><%= link_to "Add your Spot!", new_spot_path %></h1>
  <% end %>
  <%= link_to '<i class="far fa-arrow-alt-circle-left"></i>'.html_safe,  spots_path, style: 'color:#5A5A5A; font-size:40px' %>
</div>


