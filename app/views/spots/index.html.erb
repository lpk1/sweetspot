
<div class="container">
  <h1>Look at these awesome camping spots</h1>

  <ul>
    <% @spots.each do |spot| %>
    <li> <%= link_to "#{spot.name}", spot_path(spot) %>
    </li>
    <% end %>
  </ul>

  <%= link_to "Create New Spot",  new_spot_path %>

  <%= link_to "Log Out", destroy_user_session_path, method: :delete %>

</div>
